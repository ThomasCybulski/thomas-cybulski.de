{
  "name": "thomas-cybulski.de",
  "private": true,
  "description": "Personal website of Thomas Cybulski",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "gatsby": "^4.16.0",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-dark-mode": "^1.1.2",
    "gatsby-plugin-manifest": "^4.16.0",
    "gatsby-plugin-offline": "^5.16.0",
    "gatsby-plugin-react-helmet": "^5.16.0",
    "gatsby-plugin-react-svg": "^3.1.0",
    "gatsby-plugin-sass": "^5.16.0",
    "gatsby-plugin-sharp": "^4.16.1",
    "gatsby-source-filesystem": "^4.16.0",
    "gatsby-transformer-sharp": "^4.16.0",
    "nanoid": "^4.0.0",
    "node-sass": "^7.0.1",
    "prop-types": "^15.8.1",
    "react": "^16.9.0",
    "react-bootstrap": "^2.4.0",
    "react-dom": "^16.9.0",
    "react-github-btn": "^1.3.0",
    "react-helmet": "^6.1.0",
    "react-reveal": "^1.2.2",
    "react-scroll": "^1.8.7",
    "react-tilt": "^0.1.4",
    "react-typed": "^1.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "prettier": "^2.7.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ThomasCybulski/thomas-cybulski.de"
  },
  "bugs": {
    "url": "https://github.com/ThomasCybulski/thomas-cybulski.de/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  }
}
